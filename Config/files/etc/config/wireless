
config wifi-device 'MT7981_1_1'
	option type 'mtwifi'
	option phy 'ra0'
	option hwmode '11g'
	option band '2g'
	option dbdc_main '1'
	option txpower '100'
	option htmode 'HE40'
	option country 'CN'
	option mu_beamformer '1'
	option noscan '1'
	option serialize '1'
	option channel '2'

config wifi-iface 'default_MT7981_1_1'
	option device 'MT7981_1_1'
	option network 'lan'
	option mode 'ap'
	option encryption 'psk2'
	option ssid 'wifi'
	option key 'Lhabc110.'

config wifi-device 'MT7981_1_2'
	option type 'mtwifi'
	option phy 'rax0'
	option hwmode '11a'
	option band '5g'
	option dbdc_main '0'
	option txpower '100'
	option htmode 'HE160'
	option country 'CN'
	option mu_beamformer '1'
	option noscan '0'
	option serialize '1'
	option channel '52'

config wifi-iface 'default_MT7981_1_2'
	option device 'MT7981_1_2'
	option network 'lan'
	option mode 'ap'
	option encryption 'psk2'
	option ssid 'wifi_5G'
	option key 'Lhabc110.'

